			"dependsOrder": "sequence",
			"group": {
				"kind": "build",
				"isDefault": true
			},
			"presentation": {
				"reveal": "always",
				"panel": "shared",
				"showReuseMessage": true,
				"clear": false
			}
		},
		{
			"label": "Build -Release",
			"dependsOn": ["compileWasm -release", "build vite"],
