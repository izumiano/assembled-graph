				"reveal": "always",
				"panel": "shared",
				"showReuseMessage": true,
				"clear": false
			}
		},
		{
			"label": "Start Dev",
			"dependsOn": ["npm: watchExamples", "npm: typecheck"],
			"group": "build",
			"presentation": {
				"reveal": "always",
				"panel": "shared",
				"showReuseMessage": true,
				"clear": false
			}
		},
		{
			"type": "npm",
			"script": "build",
			"label": "build vite",
			"isBackground": true,
			"group": "build"
		},
		{
			"type": "npm",
			"script": "watchExamples",
			"label": "npm: watchExamples",
			"problemMatcher": "$tsc-watch",
			"isBackground": true,
			"detail": "start watch examples",
			"group": "build"
		},
		{
			"type": "npm",
			"script": "compileWasmDev",
