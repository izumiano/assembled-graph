			"group": "build",
			"presentation": {
				"clear": true
			}
		},
		{
			"type": "npm",
			"script": "compileWasm",
			"label": "compileWasm -release",
			"detail": "compile rust to wasm -release",
			"group": "build",
			"presentation": {
				"clear": true
			}
		},
		{
			"type": "npm",
			"script": "lint",
			"label": "biome: check",
			"detail": "check biome",
			"group": {
				"kind": "test",
				"isDefault": false
			}
		},
		{
			"type": "npm",
			"script": "biomeFix",
			"label": "biome: fix",
			"detail": "fix with biome",
